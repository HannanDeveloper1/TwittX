{
  "expo": {
    "scheme": "twittx",
    "userInterfaceStyle": "automatic",
    "orientation": "default",
    "web": {
      "output": "static"
    },
    "plugins": [
      [
        "expo-router",
        {
          "origin": "https://twittx.vercel.app"
        }
      ],
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#dbeafe",
          "image": "./src/assets/images/splash-icon.png",
          "dark": {
            "image": "./src/assets/images/splash-icon.png",
            "backgroundColor": "#061b28"
          },
          "imageWidth": 200
        }
      ],
      "expo-font"
    ],
    "icon": "./src/assets/images/icon.png",
    "name": "TwittX",
    "slug": "TwittX"
  }
}
